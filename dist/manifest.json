{
  "manifest_version": 2,
  "name": "Task Manager",
  "homepage_url": "https://github.com/mygithubaccount3/task-manager-browser-extension",
  "description": "A Vue Browser Extension",
  "default_locale": "en",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Task Manager",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "version": "0.1.0",
  "content_security_policy": "script-src 'self' ; object-src 'self'"
}